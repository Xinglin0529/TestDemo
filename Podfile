source 'https://github.com/CocoaPods/Specs.git'
source 'https://github.com/Xinglin0529/MYSpecs.git'

use_frameworks!
platform:ios,'9.0'

def pubPod
   pod 'RxCocoa'
   pod 'SnapKit'
   pod 'RxDataSources'
   pod 'ObjectMapper'
end

def privatePod
    pod 'MYView'
    pod 'MYTableView'
    pod 'MYList'
end

target 'TestDemo' do
    pubPod
    privatePod
end
